{
  "name": "meteor-cypress-circle-ci",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "start:e2e":
      "MONGO_URL=mongodb://localhost:27017/meteor-test-e2e meteor --port 3050",
    "test:e2e":
      "ttab -t meteor-e2e-server -d ./ 'meteor npm run start:e2e' && cypress open"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.42",
    "meteor-node-stubs": "^0.3.2"
  },
  "devDependencies": {
    "cypress": "^2.1.0",
    "ttab": "^0.6.1"
  }
}
